<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.github.sphen13.bzinstall</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Scripts/bz/backblaze-deployer.sh</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/Library/Scripts/bz/.trigger</key>
			<true/>
		</dict>
	</dict>
	<key>OnDemand</key>
	<true/>
</dict>
</plist>
